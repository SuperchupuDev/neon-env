{
  "name": "neon-env",
  "version": "0.2.3",
  "description": "Typed env parser",
  "scripts": {
    "build": "tsdown",
    "changelog": "auto-changelog -p --commit-limit false",
    "check": "biome check",
    "format": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome lint --fix --unsafe",
    "test": "node --test",
    "typecheck": "tsc"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "sideEffects": "false",
  "files": [
    "dist"
  ],
  "author": "Superchupu",
  "license": "MIT",
  "keywords": [
    "environment variables",
    "env",
    "parser",
    "env parser",
    "typescript",
    "typed",
    "config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SuperchupuDev/neon-env.git"
  },
  "bugs": {
    "url": "https://github.com/SuperchupuDev/neon-env/issues"
  },
  "homepage": "https://github.com/SuperchupuDev/neon-env#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.15.21",
    "auto-changelog": "^2.5.0",
    "tsdown": "^0.12.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "pnpm@10.11.0"
}
