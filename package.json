{
  "name": "neon-env",
  "version": "0.2.1",
  "description": "Typed env parser",
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "check": "biome check .",
    "check:apply": "biome check --apply .",
    "format": "biome format --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "changelog": "auto-changelog -p --commit-limit false"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "author": "Superchupu",
  "license": "MIT",
  "keywords": [
    "environment variables",
    "env",
    "parser",
    "env parser",
    "typescript",
    "typed",
    "config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SuperchupuDev/neon-env.git"
  },
  "bugs": {
    "url": "https://github.com/SuperchupuDev/neon-env/issues"
  },
  "homepage": "https://github.com/SuperchupuDev/neon-env#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.6.2",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "pnpm@8.15.5"
}
